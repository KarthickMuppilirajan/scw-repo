        <FIELD NAME="event_descriptionformat" TYPE="int" LENGTH="2" NOTNULL="false" DEFAULT="1" SEQUENCE="false"/>
        <FIELD NAME="event_country" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
